{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }, 
  "exclude": [
    "node_modules"
  ],
  "include": [
    "shared/**/*.ts",
    "frontend/**/*.ts",
    "frontend/**/*.tsx",
    "frontend/**/*.vue"
  ]
}
